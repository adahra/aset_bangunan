<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="zone_seq" model="ir.sequence">
            <field name="name">Sequence Zona</field>
            <field name="code">bangunan.zona.sequence</field>
            <field name="prefix">Z</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="zona_def" model="bangunan.zona">
            <field name="name">Utara</field>
            <field name="area_id" ref="area_def"/>
        </record>
    </data>
</odoo>