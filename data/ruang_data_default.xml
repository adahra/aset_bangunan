            <field name="name">Ruang Divisi Finance &amp; Accounting</field>
            <field name="bangunan_id">1</field>
            <field name="lantai_id">1</field>
        </record>

        <record id="ruang_it_def" model="bangunan.ruang">
            <field name="name">Ruang Divisi IT</field>
            <field name="bangunan_id">1</field>
            <field name="lantai_id">1</field>
